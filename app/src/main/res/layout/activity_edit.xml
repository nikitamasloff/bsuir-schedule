<?xml version="1.0" encoding="utf-8"?>
<com.yarolegovich.mp.MaterialPreferenceScreen
    android:id="@+id/schedule_preference_screen"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whiteColor"
    tools:context=".ui.edit.EditActivity">


    <LinearLayout
        style="@style/FormCategoryLayout"
        android:id="@+id/form_category_main"
        >


        <!--separator-->
        <View
            style="@style/FormDivider"
            android:layout_marginStart="0dp"
            />


        <FrameLayout
            style="@style/FormHeaderLayout"
            android:id="@+id/form_category_main_header"
            >

            <TextView
                style="@style/FormHeaderTitle"
                android:id="@+id/form_category_main_title"
                android:text="@string/form_category_main"
                />

        </FrameLayout>


        <!--separator-->
        <View
            style="@style/FormDivider"
            />


        <LinearLayout
            style="@style/FormItemLayout"
            android:id="@+id/form_subject_layout"
            >

            <TextView
                android:id="@+id/form_subject_description"
                style="@style/FormItemDescription"
                android:text="@string/form_subject" />

            <!--separator-->

            <com.rengwuxian.materialedittext.MaterialEditText
                style="@style/FormItemEditText"
                android:id="@+id/form_subject"
                android:hint="@string/form_subject"
                app:met_autoValidate="false"
                app:met_clearButton="true"
                />

        </LinearLayout>


        <!--separator-->
        <View
            style="@style/FormDivider"
            />


        <LinearLayout
            style="@style/FormItemLayout"
            android:id="@+id/form_lesson_type_layout"
            >

            <TextView
                android:id="@+id/form_lesson_type_description"
                style="@style/FormItemDescription"
                android:text="@string/form_lesson_type" />

            <TextView
                style="@style/FormItemEditText"
                android:id="@+id/form_lesson_type"
                android:hint="@string/form_lesson_type"
                />

        </LinearLayout>


        <!--separator-->
        <View
            style="@style/FormDivider"
            />


        <LinearLayout
            style="@style/FormItemLayout"
            android:id="@+id/form_subgroup_layout"
            >

            <TextView
                android:id="@+id/form_subgroup_description"
                style="@style/FormItemDescription"
                android:text="@string/form_subgroup" />

            <TextView
                style="@style/FormItemEditText"
                android:id="@+id/form_subgroup"
                android:hint="@string/form_subgroup"
                />

        </LinearLayout>

    </LinearLayout>














