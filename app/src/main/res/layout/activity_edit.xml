                style="@style/FormItemDescription"
                android:text="@string/form_note" />

            <com.rengwuxian.materialedittext.MaterialEditText
                style="@style/FormItemEditText"
                android:id="@+id/form_note"
                android:hint="@string/form_note"
                app:met_clearButton="true"
                app:met_autoValidate="false"
                />

        </LinearLayout>


        <!--separator-->
        <View
            style="@style/FormDivider"
            />


        <LinearLayout
            style="@style/FormItemLayout"
            android:id="@+id/form_color_layout"
            >

            <TextView
                android:id="@+id/form_color_description"
                style="@style/FormItemDescription"
                android:text="@string/form_color" />

            <FrameLayout
                android:id="@+id/form_color_wrapper"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_gravity="start|center_vertical"
                >

                <View
                    android:id="@+id/form_color"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    />

            </FrameLayout>

        </LinearLayout>


        <!--separator-->
        <View
            style="@style/FormDivider"
            />


    </LinearLayout>





    <View
        style="@style/FormHeaderLayout"
        />




