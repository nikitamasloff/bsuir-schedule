                style="@style/FormItemDescription"
                android:text="@string/form_group" />

            <com.rengwuxian.materialedittext.MaterialMultiAutoCompleteTextView
                style="@style/FormItemEditText"
                android:id="@+id/form_group"
                android:hint="@string/form_group"
